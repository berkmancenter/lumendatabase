<%= form_tag do %>
  <%= token_tag(nil) %>
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th class="shrink">
        <th>Title</th>
        <th>Sent</th>
        <th>Recieved</th>
        <th>Submitted</th>
        <th class="last">Priority</th>
      </tr>
    </thead>
    <tbody>
      <% @objects.each do |object| %>
        <tr class="queue_item" id="notice_<%= object.id %>">
          <td><%= check_box_tag('selected[]', object.id) %></td>
          <td><%= object.title %></td>
          <td><%= date_sent(object) %></td>
          <td><%= date_received(object) %></td>
          <td><%= object.created_at.to_s(:simple) %></td>
          <td class="last"><%= '?' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <div class="form-actions">
    <%= submit_tag 'Process selected', class: 'btn' %>
  </div>
<% end %>
